<!-- <text>{{markers[id].placeName}}</text> -->
<block wx:if='{{isshow}}'>
  <map id="map" longitude="115.8325170000" latitude="32.8907140000" scale="8" include-points="{{markers}}" markers="{{markers}}" bindmarkertap="showModal" data-id="{{markers}}" bindregionchange="regionchange" show-location style="width: 100%; height: 100%;">
    <!-- <cover-view class='index_bt1'>
      <cover-image class='xiaoer' bindtap="login" src="/images/mk.png" />
    </cover-view>
    <cover-view class='index_shuaxin'>
      <cover-image class='shuaxin' src="/images/mk.png" />
    </cover-view> -->
    <!-- 屏幕背景变暗的背景 -->
    <cover-view class="commodity_screen" bindtap="hideModal" wx:if="{{showModalStatus}}"></cover-view>
    <!-- 弹出框 -->
    <cover-view animation="{{animationData}}" class="commodity_attr_box" wx:if="{{showModalStatus}}">
      <cover-view class='placeBox'>
        <cover-view class='placeViewLt'>
          <cover-view class='viewTitle'>{{marker.name}}</cover-view>
          <cover-view class='viewDis'>{{marker.address}}</cover-view>
          <!-- <cover-view class='viewAddr'>{{marker.longitude}}</cover-view> -->
        </cover-view>
        <cover-view class='placeViewRt'>
          <cover-image data-id="{{marker.id}}" bindtap="opendetail" src='/images/{{marker.type}}.png'></cover-image>
        </cover-view>
      </cover-view>
      <cover-view class='viewIcon'>
        <cover-image class='indexIcon' src='/images/time.png'></cover-image>
        <cover-view class='timeText'>{{marker.jiedainengli}}人</cover-view>
        <cover-image class='indexIcon1' data-id="{{marker.tel}}" src='/images/share.png' bindtap='calling'></cover-image>
        <cover-view class='timeText1' data-id="{{marker.tel}}" bindtap='calling'>{{marker.tel}}</cover-view>
        <cover-image class='indexIcon2' src='/images/nav.png' bindtap='daohang'></cover-image>
        <cover-view class='timeText1' bindtap='daohang'>导航</cover-view>
      </cover-view>
    </cover-view>
  </map>
</block>